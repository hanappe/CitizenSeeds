    <div id="ForumViewer" class="container-fluid forum-container"></div>

    <div id="ExperimentView"></div>

    <!-- Bootstrap core JavaScript -->
    <script src="<%= config.baseUrl %>/jquery.js"></script>
    <script src="<%= config.baseUrl %>/bootstrap.js"></script>
    <script src="<%= config.baseUrl %>/es6-promise-2.0.1.min.js"></script>
    <script src="<%= config.baseUrl %>/CitizenSeedsApp.js"></script>
    <script src="<%= config.baseUrl %>/CitizenSeedsServer.js"></script>
    <script src="<%= config.baseUrl %>/dygraph-combined.js"></script>
