<script>
var jq = $.noConflict();
jq(document).ready(function(){
    _server = new Server("<%= config.baseUrl %>");
    <% if (account && account.id) { %>
    _account = { "id": "<%= account.id %>" };
    <% } %>
    showObservations(<%= experiment.id %>, "<%= startAt %>");
    initForum(<%= experiment.id %>);
    showRecentActivity(<%= experiment.id %>);
})
jq("#navbar-experiments-<%= experiment.id %>").addClass("active");
</script>

  </body>
</html>
